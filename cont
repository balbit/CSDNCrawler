    $(function(){
        $(document).on('click',"a.option-box[data-type='report']",function() {
            window.csdn.loginBox.key({
              biz: 'blog',
              subBiz: 'other_service',
              cb: function(){
                window.csdn.feedback({
                  "type":'blog',
                  "rtype":'article',
                  "rid":articleId,
                  "reportedName":username,
                  "submitOptions":{
                    "title":articleTitle,
                    "contentUrl":articleDetailUrl
                  },
                  "callback":function(){
                    showToast({
                      text: "感谢您的举报，我们会尽快审核！",
                      bottom: '10%', 
                      zindex: 9000, 
                      speed: 500,
                      time: 1500
                    })
                  }
                })
              }
            })
        });
    })
